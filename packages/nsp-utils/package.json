{
  "name": "nsp-utils",
  "private": true,
  "version": "2.1.0",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier -c --write \\\"src/**/*\\\"",
    "tsc": "tsc --noEmit --skipLibCheck --incremental",
    "lint": "eslint --ext .jsx,.js,.tsx,.ts src/**/*",
    "lint:error": "eslint --quiet --ext .jsx,.js,.tsx,.ts src/**/*",
    "clean": "rimraf .turbo && rimraf node_modules && rimraf dist && rimraf docs && rimraf wiki",
    "docs": "typedoc",
    "wiki": "typedoc --options typedoc.wiki.json --plugin typedoc-plugin-markdown --plugin typedoc-github-wiki-theme"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "@types/lodash": "^4.17.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "axios": "^1.7.4",
    "lodash": "^4.17.21",
    "nsp-lint": "workspace:^",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "typedoc": "^0.24.8",
    "typedoc-github-wiki-theme": "^1.1.0",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.5.4",
    "vite": "^4.5.3"
  },
  "dependencies": {
    "store2": "^2.14.3"
  }
}
